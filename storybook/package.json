{
  "name": "storybook",
  "version": "1.0.0",
  "description": "Nuxt.js + Storybook for components",
  "author": "Bezkrovnyi Anton",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-gh-pages": "yarn install && yarn build-storybook && push-dir --dir=storybook-static --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "@rabota/rabotify": "^1.4.49",
    "nuxt": "^2.0.0",
    "push-dir": "^0.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/addon-storysource": "^5.2.1",
    "@storybook/addons": "^5.2.1",
    "@storybook/vue": "^5.2.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "fast-sass-loader": "^1.5.0",
    "storybook-addon-vue-info": "^1.3.2",
    "storybook-vue-router": "^1.0.7"
  }
}
